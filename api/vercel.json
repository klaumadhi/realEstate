{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/auth(.*)", "dest": "/api/routes/auth.route.js" },
    { "src": "/api/posts(.*)", "dest": "/api/routes/posts.route.js" },
    { "src": "/api/users(.*)", "dest": "/api/routes/users.route.js" },
    { "src": "/api/test(.*)", "dest": "/api/routes/test.route.js" },
    { "src": "/api/chats(.*)", "dest": "/api/routes/chats.route.js" },
    { "src": "/api/messages(.*)", "dest": "/api/routes/messages.route.js" }
  ]
}
